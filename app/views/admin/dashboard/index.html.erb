<div class="row">
  <div class="col-md-12">
    <div class="content-box-large">
      <div class="panel-heading">
        <div class="panel-title"><%= t ".recent_bookings" %></div>
      </div>
      <div class="panel-body">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>#</th>
              <th><%= t ".tour_name" %></th>
              <th><%= t ".user_name" %></th>
              <th><%= t ".booked_at" %></th>
              <th><%= t ".status" %></th>
              <th><%= t ".action" %></th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: "booking", collection: @recent_bookings, as: :booking %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
